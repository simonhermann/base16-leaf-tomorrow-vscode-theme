  "tokenColors": [
    {
      "settings": {
        "foreground": "#c5c8c6ff",
      }
    },
    {
      "name": "Classes",
      "scope": "meta.class",
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Text",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#969896",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.variable",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.definition.block",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.template.expression",
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#81ac6f"
      }
    },
    {
      "name": "Object Property Names",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "support.class",
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Named Type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Support Constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Primitive/Builtin Type",
      "scope": [
        "support.type.primitive",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Method Definition",
      "scope": "meta.definition.method",
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "meta.function-call",
        "meta.require",
        "variable.language.super",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#81a2be"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Support",
      "scope": "support.function",
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Markdown Link Brackets",
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Floats",
      "scope": "none",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Boolean",
      "scope": "none",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#de935f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#81ac6f"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#81a2be"
      }
    },
    {
      "name": "Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Decorator",
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Values",
      "scope": "none",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "CSS Property Name",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": "support.constant.property-value.css",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "entity.name.section"
      ],
      "settings": {
        "fontStyle": "bold underline",
        "foreground": "#81ac6f"
      }
    },
    {
      "name": "Headings punctuation",
      "scope": [
        "markup.heading punctuation.definition.heading"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#555555"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Bold",
      "scope": "punctuation.definition.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Bold punctuation",
      "scope": [
        "punctuation.definition.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#555555"
      }
    },
    {
      "name": "Italic",
      "scope": "punctuation.definition.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Italic punctuation",
      "scope": [
        "punctuation.definition.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#555555"
      }
    },
    {
      "name": "Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#969896",
      }
    },
    {
      "name": "Link",
      "scope": [
        "meta.link",
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#969896"
      }
    },
    {
      "name": "Markdown Link Punctuation",
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#969896"
      }
    },
    {
      "name": "Link",
      "scope": [
        "string.other.link",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Lists",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#81ac6f"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#c5c8c6",
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#81ac6f"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Colors",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "variable.interpolation"
      ],
      "settings": {
        "foreground": "#a3685a"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#1d1f21"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
