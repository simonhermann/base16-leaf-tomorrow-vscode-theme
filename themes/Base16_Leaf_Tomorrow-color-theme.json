    "tab.activeForeground": "#d9dbde",
    "tab.inactiveBackground": "#161719",
    "titleBar.activeBackground": "#161719",
    "titleBar.border": "#030404",
    "titleBar.inactiveBackground": "#161719",
    "widget.shadow": "#16161638",
